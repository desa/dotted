# Put any aliases you want in your environment here


# Aliases that depend on whether we are running bash or zsh
if [ "$SHELL" == "$(which bash)" ]; then
	alias reld='source ~/.bash_profile'
elif [ "$SHELL" == "$(which zsh)" ]; then
	unalias gcm
	
	alias reld='source ~/.zshrc'
	alias which-command=whence
	alias zshconfig='edit ~/.zshrc'
	alias ohmyzsh='edit ~/.oh-my-zsh'
fi

# Global aliases to define
alias -g ......=../../../../..
alias -g .....=../../../..
alias -g ....=../../..
alias -g ...=../..

